{
	"name": "jacamo4code",
	"displayName": "JaCaMo4Code",
	"description": "JaCaMo syntax highlighting and code snippets for VS Code.",
	"version": "1.2.1",
	"publisher": "tabajara-krausburg",
	"engines": {
		"vscode": "^1.45.0"
	},
	"icon": "img/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/TabajaraKrausburg/JaCaMo4Code"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Themes"
	],
	"contributes": {
		"languages": [
			{
				"id": "mas2j",
				"aliases": [
					"mas2j"
				],
				"extensions": [
					".mas2j"
				]
			},
			{
				"id": "jason",
				"aliases": [
					"Jason",
					"asl"
				],
				"extensions": [
					".asl"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "jcm",
				"aliases": [
					"JaCaMo",
					"jcm"
				],
				"extensions": [
					".jcm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "mas2j",
				"scopeName": "source.mas2j",
				"path": "./syntaxes/mas2j.tmLanguage.json"
			},
			{
				"language": "jason",
				"scopeName": "source.jason",
				"path": "./syntaxes/jason.tmLanguage.json"
			},
			{
				"language": "jcm",
				"scopeName": "source.jcm",
				"path": "./syntaxes/jcm.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "jason",
				"path": "./snippets/asl.json"
			},
			{
				"language": "xml",
				"path": "./snippets/moise.json"
			}
		],
		"themes": [
			{
				"label": "JaCaMo Theme",
				"uiTheme": "vs",
				"path": "./themes/jacamo.color-theme.json"
			}
		]
	}
}
