			},
			{
			   "id": "jason",
			   "aliases": ["Jason", "asl"],
			   "extensions": [".asl"],
			   "configuration": "./language-configuration.json"
			},
			{
                "id": "jcm",
                "aliases": ["JaCaMo", "jcm"],
                "extensions": [".jcm"],
                "configuration": "./language-configuration.json"
            }
		 ],
		 "grammars": [
			{
			   "language": "mas2j",
			   "scopeName": "mas2j",
			   "path": "./syntaxes/mas2j.tmLanguage.json"
			},
			{
			   "language": "jason",
			   "scopeName": "jason",
			   "path": "./syntaxes/jason.tmLanguage.json"
			},
			{
				"language": "jcm",
				"scopeName": "source.jcm",
				"path": "./syntaxes/jcm.tmLanguage.json"
			}
		 ],
		"snippets": [
			{
			  "language": "jason",
			  "path": "./snippets/asl.json"
			}
		]
	}
